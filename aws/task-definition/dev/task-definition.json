{
    "executionRoleArn": "arn:aws:iam::783354560127:role/TaskExecutionRole",
    "containerDefinitions": [
        {
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/Viite-dev",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "portMappings": [
                {
                    "hostPort": 9080,
                    "protocol": "tcp",
                    "containerPort": 9080
                }
            ],
            "cpu": 0,
            "environment": [
                { "name": "userProvider", "value": "fi.liikennevirasto.digiroad2.dao.OracleUserProvider" },
                { "name": "municipalityProvider", "value": "fi.liikennevirasto.digiroad2.dao.OracleMunicipalityProvider" },
                { "name": "eventBus", "value": "fi.liikennevirasto.digiroad2.DigiroadEventBus" },
                { "name": "vvhServiceHost", "value": "haproxy.vayla.fi" },
                { "name": "vvhRestApiEndPoint", "value": "http://haproxy.vayla.fi:2024/vvhdata/" },
                { "name": "vvhRoadlink.frozen", "value": "true" },
                { "name": "vkmUrl", "value": "http://oag.liikennevirasto.fi" },
                { "name": "tierekisteriViiteRestApiEndPoint", "value": "" },
                { "name": "tierekisteri.enabled", "value": "false" },
                { "name": "viite.importTimeStamp", "value": "1545091200000" },
                { "name": "http.proxySet", "value": "false" },
                { "name": "http.proxyHost", "value": "127.0.0.1" },
                { "name": "http.nonProxyHosts", "value": "127.0.0.1" },
                { "name": "importOnlyCurrent", "value": "false" },
                { "name": "authenticationTestMode", "value": "false" },
                { "name": "bonecp.jdbcUrl", "value": "jdbc:postgresql://vd1g916wf1lodch.c8dj2qlvf50d.eu-west-1.rds.amazonaws.com:5432/viite_dev" },
                { "name": "bonecp.username", "value": "viite_dev" },
                { "name": "import.bonecp.jdbcUrl", "value": "jdbc:postgresql://vd1g916wf1lodch.c8dj2qlvf50d.eu-west-1.rds.amazonaws.com:5432/drkonv" },
                { "name": "import.bonecp.username", "value": "drkonv" },
                { "name": "conversion.bonecp.jdbcUrl", "value": "jdbc:postgresql://vd1g916wf1lodch.c8dj2qlvf50d.eu-west-1.rds.amazonaws.com:5432/drkonv" },
                { "name": "conversion.bonecp.username", "value": "drkonv" },
                { "name": "authentication.basic.username", "value": "kalpa" },
                { "name": "authentication.admin.basic.username", "value": "admin" },
                { "name": "authentication.serviceRoad.basic.username", "value": "livi" },
                { "name": "authentication.municipality.basic.username", "value": "municipality" },
                { "name": "viiteTierekisteri.username", "value": "trrestviite" },
                { "name": "env", "value": "Dev" }
            ],
            "mountPoints": [],
            "secrets": [
                {
                    "name": "bonecp.password",
                    "valueFrom": "arn:aws:ssm:eu-west-1:783354560127:parameter/dev/bonecp.password"
                },
                {
                    "name": "import.bonecp.password",
                    "valueFrom": "arn:aws:ssm:eu-west-1:783354560127:parameter/dev/import.bonecp.password"
                },
                {
                    "name": "conversion.bonecp.password",
                    "valueFrom": "arn:aws:ssm:eu-west-1:783354560127:parameter/dev/conversion.bonecp.password"
                },
                {
                    "name": "authentication.basic.password",
                    "valueFrom": "arn:aws:ssm:eu-west-1:783354560127:parameter/dev/authentication.basic.password"
                },
                {
                    "name": "authentication.admin.basic.password",
                    "valueFrom": "arn:aws:ssm:eu-west-1:783354560127:parameter/dev/authentication.admin.basic.password"
                },
                {
                    "name": "authentication.serviceRoad.basic.password",
                    "valueFrom": "arn:aws:ssm:eu-west-1:783354560127:parameter/dev/authentication.serviceRoad.basic.password"
                },
                {
                    "name": "authentication.municipality.basic.password",
                    "valueFrom": "arn:aws:ssm:eu-west-1:783354560127:parameter/dev/authentication.municipality.basic.password"
                },
                {
                    "name": "viiteTierekisteri.password",
                    "valueFrom": "arn:aws:ssm:eu-west-1:783354560127:parameter/dev/viiteTierekisteri.password"
                }
            ],
            "memoryReservation": 2048,
            "volumesFrom": [],
            "image": "783354560127.dkr.ecr.eu-west-1.amazonaws.com/viite:latest",
            "name": "viite",
            "healthCheck": {
                "startPeriod": 15,
                "command": [ "CMD-SHELL", "curl -f http://127.0.0.1:9080/api/ping/ || exit 1" ]
            }
        }
    ],
    "memory": "4096",
    "taskRoleArn": "arn:aws:iam::783354560127:role/TaskExecutionRole",
    "family": "Viite-dev",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "2048",
    "inferenceAccelerators": [],
    "volumes": [],
    "tags": [
        {
            "key": "Application",
            "value": "Viite"
        }
    ]
