#!/bin/bash
#
# Start up script for the VIITE server
#
# chkconfig: 345 85 15
# description: VIITE

case "$1" in
  start)
    runuser web -lc $"
      cd /home/web/ci-test/current
            ./start.sh &"
        ;;
  stop)
    cd /home/web/ci-test/current
    ./stop.sh || true
        ;;
  *)
          echo $"Usage: $0 {start|stop}"
          exit 2
esac
